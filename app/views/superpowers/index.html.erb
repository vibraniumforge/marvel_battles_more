<h1>List of all Superpowers</h1>
<br>
<%= link_to "New Superpower", new_superpower_path(@superpower) %>
<br>
<table>
  <thead>
    <tr>
      <th>Superpower</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
      <% @superpowers.each do |superpower| %>
        <tr><td><%= link_to superpower.name, superpower_path(superpower.id) %></td></tr>
      <% end %>
  </tbody>
</table>

